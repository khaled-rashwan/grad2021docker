@model IEnumerable<grad2021.Models.InstructorProfession>

@{
    ViewData["Title"] = "Index";
}

<h1>Index</h1>

<p>
    <a asp-action="Create">Create New</a>
</p>
<table class="table">
    <thead>
        <tr>
            <th>
                @Html.DisplayNameFor(model => model.ProfessionDegree)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.PromotionDate)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.Instructor)
            </th>
            <th></th>
        </tr>
    </thead>
    <tbody>
@foreach (var item in Model) {
        <tr>
            <td>
                @Html.DisplayFor(modelItem => item.ProfessionDegree)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.PromotionDate)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.Instructor.InstructorNatId)
            </td>
            <td>
                <a asp-action="Edit" asp-route-id="@item.InstructorProfessionID">Edit</a> |
                <a asp-action="Details" asp-route-id="@item.InstructorProfessionID">Details</a> |
                <a asp-action="Delete" asp-route-id="@item.InstructorProfessionID">Delete</a>
            </td>
        </tr>
}
    </tbody>
</table>
